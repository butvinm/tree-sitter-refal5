      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "([a-zA-Z_][a-zA-Z0-9_-]*)|([0-9]+)"
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    },
    "compound": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\.)*\""
    },
    "macrodigit": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "chars": {
      "type": "PATTERN",
      "value": "'([^'\\\\]|\\\\.)*'"
    },
    "block_comment": {
